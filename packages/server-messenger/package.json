{
    "name": "@privateaim/server-messenger",
    "type": "module",
    "version": "0.8.22",
    "private": true,
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs"
        }
    },
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "license": "Apache-2.0",
    "description": "This package contains the realtime application which connects the API with socket based clients.",
    "dependencies": {
        "@authup/kit": "^1.0.0-beta.28",
        "@authup/core-kit": "^1.0.0-beta.28",
        "@ebec/http": "^2.3.0",
        "@privateaim/messenger-kit": "^0.7.23",
        "@privateaim/server-kit": "^0.8.22",
        "@privateaim/server-telemetry-kit": "^0.8.22",
        "@privateaim/server-http-kit": "^0.8.22",
        "@privateaim/server-realtime-kit": "^0.8.22",
        "@privateaim/telemetry-kit": "^0.8.22",
        "dotenv": "^17.2.3",
        "envix": "^1.3.0",
        "hapic": "^2.7.0",
        "redis-extension": "^2.0.4",
        "routup": "^4.0.2",
        "socket.io": "^4.8.2"
    },
    "scripts": {
        "dev": "ts-node src/index.ts",
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "rimraf ./dist && cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "start": "node dist/index.mjs"
    }
}
