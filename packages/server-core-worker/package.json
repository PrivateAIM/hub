{
    "name": "@privateaim/server-core-worker",
    "type": "module",
    "version": "0.8.22",
    "private": true,
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs"
        }
    },
    "author": {
        "name": "Peter Placzek",
        "email": "tadashi@animesu.net",
        "url": "https://tada5hi.net"
    },
    "license": "Apache-2.0",
    "description": "This project contains the core worker service.",
    "scripts": {
        "lint": "eslint --ext .ts src/",
        "lint:fix": "npm run lint -- --fix",
        "start": "node dist/index.mjs",
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "rimraf ./dist && cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "test": "cross-env NODE_ENV=test vitest run --config test/vitest.config.ts",
        "test:coverage": "cross-env NODE_ENV=test vitest run --config test/vitest.config.ts --coverage"
    },
    "dependencies": {
        "@authup/core-http-kit": "^1.0.0-beta.28",
        "@ebec/http": "^2.3.0",
        "@hapic/harbor": "^2.5.4",
        "@privateaim/core-kit": "^0.8.22",
        "@privateaim/kit": "^0.8.22",
        "@privateaim/core-http-kit": "^0.8.22",
        "@privateaim/telemetry-kit": "^0.8.22",
        "@privateaim/server-kit": "^0.8.22",
        "@privateaim/server-telemetry-kit": "^0.8.22",
        "@privateaim/server-core-worker-kit": "^0.7.23",
        "@privateaim/storage-kit": "^0.8.22",
        "@validup/adapter-zod": "^0.1.12",
        "amqp-extension": "^4.0.0",
        "dockerode": "^4.0.9",
        "docken": "^2.0.2",
        "dotenv": "^17.2.3",
        "envix": "^1.3.0",
        "gunzip-maybe": "^1.4.2",
        "hapic": "^2.7.0",
        "routup": "^4.0.2",
        "rapiq": "^0.9.0",
        "redis-extension": "^2.0.4",
        "singa": "^1.0.0",
        "tar": "^7.5.2",
        "tar-fs": "^3.1.1",
        "tar-stream": "^3.1.6",
        "uuid": "^13.0.0",
        "validup": "^0.1.9",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@types/dockerode": "^4.0.0",
        "@types/tar-fs": "^2.0.4",
        "@types/tar-stream": "^3.1.4",
        "@types/uuid": "^11.0.0",
        "ts-node": "^10.9.2",
        "vitest": "^4.0.14"
    },
    "publishConfig": {
        "access": "public"
    }
}
